  function MyApp(props){
        return (
            <div>
            	 <h1>{props.title}</h1>
                <h2>Good Morning: {props.numItems}</h2>
            </div>

        ) 
    }
    













    ReactDOM.render(
        <MyApp title = "Welcome to World Of ReactJs" numItems = "John"/>,
        document.getElementById("root")
    )